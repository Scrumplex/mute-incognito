{
	"manifest_version": 2,
	"name": "Mute Incognito",
	"short_name": "Mute Incognito",
	"description": "Mutes new tabs that are in incognito mode",
	"version": "1.0.2",
	"minimum_chrome_version": "55",
	"background": {
		"scripts": [
			"mute_tabs.js"
		],
		"persistent": true
	},
	"permissions": [
		"tabs"
	]
}
